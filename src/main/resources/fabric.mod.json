{
  "schemaVersion": 1,
  "id": "mineraddon",
  "version": "${version}",
  "name": "Miner Addon",
  "description": "A Meteor Addon for mining game.",
  "authors": [
    "Tympanicblock61"
  ],
  "contact": {
    "repo": "https://github.com/tympanicblock61/mineraddon"
  },
  "icon": "assets/miner/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.tympanic.mineraddon.AddonTemplate"
    ]
  },
  "mixins": [
    "mineraddon.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["1.20.5","${mc_version}"],
    "meteor-client": "*"
  }
}
